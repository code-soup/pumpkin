{
    "name": "codesoup/pumpkin",
    "type": "wordpress-theme",
    "license": "MIT",
    "description": "Component oriented WordPress starter theme with a modern development workflow",
    "homepage": "https://www.codesoup.co/pumpkin/",
    "authors": [
        {
            "name": "Vlado Bosnjak",
            "email": "hi@codesoup.co",
            "homepage": "https://www.codesoup.co"
        }
    ],
    "keywords": [
        "wordpress"
    ],
    "support": {
        "issues": "https://github.com/code-soup/pumpkin/issues"
    },
    "autoload": {
        "psr-4": {
            "CS\\": "inc/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://pivvenit.github.io/acf-composer-bridge/composer/v3/library/"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "composer/installers": "~1.0",
        "soberwp/models": "1.1.0-p",
        "advanced-custom-fields/advanced-custom-fields-pro": "^5.9"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}": [
                "vendor/package"
            ]
        }
    }
}
