{
    "name": "codesoup/pumpkin",
    "type": "wordpress-theme",
    "license": "MIT",
    "description": "Component oriented WordPress starter.",
    "homepage": "https://www.codesoup.co/pumpkin/",
    "authors": [
        {
            "name": "Vlado Bosnjak",
            "email": "hi@codesoup.co",
            "homepage": "https://www.codesoup.co"
        }
    ],
    "keywords": [
        "wordpress"
    ],
    "support": {
        "issues": "https://github.com/code-soup/pumpkin/issues"
    },
    "autoload": {
        "psr-4": {
            "CS\\": "inc/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://connect.advancedcustomfields.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "composer/installers": "~1.0",
        "soberwp/models": "1.1.0-p",
        "wpengine/advanced-custom-fields-pro": "*"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}": [
                "vendor/package",
                "type:wordpress-plugin"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
